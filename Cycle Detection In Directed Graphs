bool dfs(int v)     {
    vis[v] = 1;
    for(int child : arr[v]) {
        if(vis[child] == 0) {
            if(dfs(child) == true)  {
                return true;
            }
        }
        else    {
            if(vis[child] == 1) {
                return true;
            }
        }
    }
    vis[v] = 2;
    return false;
}
